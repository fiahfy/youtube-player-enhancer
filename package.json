{
  "name": "youtube-player-enhancer",
  "description": "Chrome Extension for Player Enhancer on YouTube.",
  "version": "0.0.6",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.2",
    "@reduxjs/toolkit": "^1.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "redux-persist-webextension-storage": "^1.0.2"
  },
  "devDependencies": {
    "@fiahfy/eslint-config-react": "^0.0.18",
    "@fiahfy/prettier-config": "^0.0.2",
    "@fiahfy/tsconfig": "^0.0.1",
    "@types/chrome": "^0.0.206",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-redux": "^7.1.25",
    "@types/redux-persist-webextension-storage": "^1.0.0",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.31.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "mv3-hot-reload": "^0.2.7",
    "prettier": "^2.8.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "private": true,
  "productName": "Player Enhancer for YouTube",
  "scripts": {
    "build": "webpack",
    "dev": "concurrently npm:watch:*",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "lint:prettier": "prettier -c --ignore-path .gitignore .",
    "package": "npm run build && mkdir -p dist && zip -r dist/archive.zip app",
    "prepare": "husky install",
    "test": "npm run lint",
    "watch:dist": "mv3-hot-reload",
    "watch:src": "webpack -w --config webpack.config.dev.js"
  }
}
