{
  "name": "youtube-player-enhancer",
  "description": "Chrome Extension for Player Enhancer on YouTube.",
  "version": "0.0.6",
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.3",
    "@reduxjs/toolkit": "^1.9.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "redux-persist-webextension-storage": "^1.0.2"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.16",
    "@fiahfy/eslint-config-react": "^0.0.20",
    "@fiahfy/prettier-config": "^0.0.3",
    "@fiahfy/tsconfig": "^0.0.2",
    "@types/chrome": "^0.0.235",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.3",
    "@types/react-redux": "^7.1.25",
    "@types/redux-persist-webextension-storage": "^1.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "private": true,
  "productName": "Player Enhancer for YouTube",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier -c --ignore-path .gitignore .",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "package": "zip -r extension.zip dist",
    "prepare": "husky install",
    "test": "npm run format && npm run lint"
  },
  "type": "module"
}
